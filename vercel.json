{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts"
    }
  ],
  "env": {
    "DB_HOST": "@db_host",
    "DB_PORT": "@db_port", 
    "DB_USERNAME": "@db_username",
    "DB_PASSWORD": "@db_password",
    "DB_NAME": "@db_name",
    "DB_SSL": "@db_ssl",
    "JWT_SECRET": "@jwt_secret"
  }
}